{
  "cSpell.words": [
    "animejs",
    "autofree",
    "bisterixstudio",
    "bitmask",
    "cmdln",
    "Color",
    "containsn",
    "deployctl",
    "Dialog",
    "dlog",
    "dotenv",
    "englebert",
    "extrabold",
    "gdignore",
    "gdscript",
    "gifsicle",
    "glog",
    "gutignore",
    "lanczos",
    "maskable",
    "neighbor",
    "npcs",
    "onready",
    "palettegen",
    "paletteuse",
    "printerr",
    "printwarn",
    "stylebox",
    "titlebar"
  ],
  "files.associations": {
    "*.djson": "json",
    "*.ds": "json"
  },
  "editor.renderWhitespace": "all",
  "deno.enable": true,
  "deno.lint": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "css.customData": [
    ".vscode/tailwind.json"
  ],
  "markdown.extension.toc.updateOnSave": false,
  "github.copilot.enable": {
    "*": false
  }
}
